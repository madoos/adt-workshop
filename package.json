		"reveal-md": "^2.4.1",
		"watch": "^1.0.2",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.1",
		"webpack-dev-server": "^3.3.1"
	},
	"dependencies": {
		"csv-stream": "^0.2.0",
		"fantasy-land": "^4.0.1",
		"fast-csv": "^2.5.0",
		"lit-html": "^1.0.0",
